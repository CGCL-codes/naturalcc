{
  "problemMatcher": [
    {
      "owner": "flake8",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^([^:]*):(\\d+):(\\d+): ([WEF]\\d\\d\\d) (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    }
  ]
}
